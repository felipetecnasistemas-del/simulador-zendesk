{
  "functions": {
    "api/scope-items.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/api/scope-items/(.*)",
      "destination": "/api/scope-items?id=$1"
    },
    {
      "source": "/simulador",
      "destination": "/simulador.html"
    }
  ]
}