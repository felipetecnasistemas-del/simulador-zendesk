{
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/api/users",
      "destination": "/api/users.js"
    },
    {
      "source": "/api/projects(.*)",
      "destination": "/api/projects.js"
    },
    {
      "source": "/api/scope-items",
      "destination": "/api/scope-items.js"
    },
    {
      "source": "/api/scope-items/(.*)",
      "destination": "/api/scope-items.js?id=$1"
    },

    {
      "source": "/api/default-projects(.*)",
      "destination": "/api/default-projects.js"
    },
    {
      "source": "/simulador",
      "destination": "/simulador.html"
    }
  ]
}