{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/users",
      "dest": "/api/users.py"
    },
    {
      "src": "/api/projects(.*)",
      "dest": "/api/projects.py"
    },
    {
      "src": "/simulador",
      "dest": "/simulador.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}