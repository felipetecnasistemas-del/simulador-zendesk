{
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/api/users",
      "destination": "/api/users.py"
    },
    {
      "source": "/api/projects(.*)",
      "destination": "/api/projects.py"
    },
    {
      "source": "/api/scope-items(.*)",
      "destination": "/api/scope-items-flask.py"
    },
    {
      "source": "/api/default-projects(.*)",
      "destination": "/api/default-projects.py"
    },
    {
      "source": "/simulador",
      "destination": "/simulador.html"
    }
  ]
}