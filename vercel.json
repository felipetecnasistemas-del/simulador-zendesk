{
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.{css,js,json}",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/simulador",
      "destination": "/simulador.html"
    }
  ]
}