{
  "version": 2,
  "functions": {
    "api/users.py": {
      "runtime": "python3.12"
    },
    "api/projects.py": {
      "runtime": "python3.12"
    }
  },
  "rewrites": [
    {
      "source": "/api/users",
      "destination": "/api/users.py"
    },
    {
      "source": "/api/projects(.*)",
      "destination": "/api/projects.py"
    },
    {
      "source": "/simulador",
      "destination": "/simulador.html"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}